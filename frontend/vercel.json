{
  "version": 2,
  "name": "pushpendra-kumar-tiwari-app",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_SITE_TITLE": "Pushpendra Kumar Tiwari",
    "VITE_SITE_BRAND": "Pushpendra Kumar Tiwari"
  }
}